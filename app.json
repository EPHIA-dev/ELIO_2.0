{
  "expo": {
    "name": "ELIO_Revolution",
    "slug": "ELIO_Revolution",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elio.revolution",
      "config": {
        "googleMapsApiKey": "YOUR_IOS_API_KEY"
      },
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "ELIO a besoin de votre position pour vous localiser sur la carte",
        "NSLocationAlwaysUsageDescription": "ELIO a besoin de votre position pour vous localiser sur la carte"
      }
    },
    "android": {
      "package": "com.elio.revolution",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "INTERNET",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "YOUR_ANDROID_API_KEY"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "ELIO a besoin de votre position pour vous localiser sur la carte"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "92df36c9-7cc4-4278-b4bd-2c1df969b5c8"
      }
    },
    "owner": "mathoufle",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/92df36c9-7cc4-4278-b4bd-2c1df969b5c8"
    }
  }
}
